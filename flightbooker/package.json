{
  "name": "narratory-flight-booker",
  "version": "0.2.1",
  "description": "A flight-booking example app for Narratory. Check out narratory.io for more examples and docs.",
  "main": "out/index.js",
  "watch": {
    "updateEntities": {
      "patterns": [
        "src"
      ],
      "ignore": "src/user.ts",
      "quiet": true,
      "extensions": "ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc && node out/cmd/bootstrap.js",
    "update": "tsc && node out/cmd/update.js",
    "chat": "node out/cmd/chat.js",
    "updateEntities": "node node_modules/narratory/out/scanner.js",
    "watch": "concurrently \"npm-watch\" \"tsc --watch\""
  },
  "author": "luddilo",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^12.12.21",
    "narratory": "^0.4.0",
    "npm-watch": "^0.6.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "typescript": "^3.7.4"
  }
}
